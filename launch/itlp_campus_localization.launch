<launch>
  <arg name="scene_name" default="2n8kARJN3HM"/>
  <arg name="path_to_gt_map" default="/home/kirill/TopoSLAM/GT/$(arg scene_name)"/>
  <arg name="path_to_load_graph" default="/media/kirill/7CM/ITLP_campus_bags/topo_graph_for_tracks"/>
  <arg name="config_file" default="itlp_campus_localization.yaml"/>

  <node pkg="prism_topomap" type="prism_topomap_node.py" name="prism_topomap_node" output="screen">
    <param name="path_to_gt_map" value="$(arg path_to_gt_map)"/>
    <param name="path_to_load_graph" value="$(arg path_to_load_graph)"/>
    <param name="config_file" value="$(arg config_file)"/>
  </node>

  <node pkg="tf" type="static_transform_publisher" name="tf_map_to_odom" args="-23.8 -17.6 0 0 0 0 map odom 100"/>
</launch>